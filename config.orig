[DB]
MYSQL_HOST = localhost
MYSQL_PORT = 3306
MYSQL_USER = pm
MYSQL_PASSWORD = password
MYSQL_DB = photo_mapper

[SECURITY]
SECRET_KEY = secret-key

[STORAGE]
# only used if S3 disabled at runtime:
UPLOAD_FOLDER = photo_mapper/static/img
S3_BUCKET = my-bucket
# url path to bucket:
S3_URL = "https://s3-us-west-2.amazonaws.com/my-bucket/"
# url path if using local storage:
LOCAL_URL = "/static/img"
# True: write to s3; False: store to local storage, use LOCAL_URL:
USE_S3 = False

[GMAPS]
KEY = my-gmaps-key

[WEB]
PORT = 5000
LISTEN_ADDRESS = 0.0.0.0
